<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import com.adventure.components.ConsoleWrapper?>

<VBox fx:id="box" alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml" fx:controller="com.adventure.controllers.RoomController" styleClass="menu-container">

    <stylesheets>
        <URL value="@../styles/menu.css" />
    </stylesheets>

    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Button fx:id="backButton" text="BACK" onAction="#back" styleClass="back-button" />

    <Label fx:id="currentRoom" styleClass="menu-label"/>

    <ConsoleWrapper text=""/>

</VBox>
